# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_AGENT_ID=your_bedrock_agent_id
BEDROCK_AGENT_ALIAS_ID=TSTALIASID

# Step Functions
STEP_FUNCTION_ARN=arn:aws:states:us-east-1:123456789012:stateMachine:MultiAgentOrchestrator

# SQS/SNS
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/multi-agent-queue
SNS_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:multi-agent-notifications

# DynamoDB
DYNAMODB_TABLE_NAME=multi-agent-state
DYNAMODB_ENDPOINT_URL=

# S3
S3_BUCKET_NAME=multi-agent-logs
S3_LOG_PREFIX=logs/

# Application
APP_NAME=MultiAgentPlatform
APP_VERSION=1.0.0
LOG_LEVEL=INFO
ENVIRONMENT=development

# VPC Configuration (for production)
VPC_ID=vpc-12345678
SUBNET_IDS=subnet-12345678,subnet-87654321
SECURITY_GROUP_ID=sg-12345678